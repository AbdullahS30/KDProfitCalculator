{
    "version": 2,
    "builds": [
      {
        "src": "server/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "client/index.html",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/public" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "dist/public/$1"
      }
    ]
  }